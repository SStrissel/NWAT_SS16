<Window x:Class="NWAT_SS16.Import"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Import" Height="412.306" Width="379" Icon="Resources/Laundry_12-512.png">
    <Grid Margin="0,0,-3,50">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20*"/>
            <ColumnDefinition Width="130*"/>
            <ColumnDefinition Width="37*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="listProjekte" Height="180" Margin="0,25,0,0" VerticalAlignment="Top" SelectionChanged="ListBox_iauswahl" Grid.Column="1" Grid.ColumnSpan="2" HorizontalAlignment="Left" Width="295"/>
        <Button Content="abbrechen" Margin="10,243,0,0" Click="abbrechen_Click" Grid.Column="1" Height="23" VerticalAlignment="Top" HorizontalAlignment="Left" Width="104"/>
        <Button Content="importieren" Margin="165,243,0,0" Click="importieren_Click" Grid.Column="1" Grid.ColumnSpan="2" Height="25" VerticalAlignment="Top" HorizontalAlignment="Left" Width="109"/>
        <Button x:Name="import_file" Content="importiere Datei" Grid.Column="1" HorizontalAlignment="Left" Margin="3,285,0,0" VerticalAlignment="Top" Width="147" Click="import_file_Click"/>
        <Button x:Name="export_file" Content="export in Datei" Grid.Column="1" HorizontalAlignment="Left" Margin="167,286,0,0" VerticalAlignment="Top" Width="147" Click="export_file_Click" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
